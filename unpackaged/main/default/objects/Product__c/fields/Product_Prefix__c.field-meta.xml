<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Product_Prefix__c</fullName>
    <description>Only used for categorizing Products on Reports</description>
    <externalId>false</externalId>
    <formula>IF(OR(BEGINS(Name, &quot;PA&quot;),BEGINS(Name, &quot;SG&quot;),BEGINS(Name, &quot;MT&quot;),BEGINS(Name, &quot;SD&quot;)), LEFT(Name, 2), IF(BEGINS(Name, &quot;GNC&quot;), LEFT(Name, 3), &quot;&quot;))</formula>
    <label>Product # Prefix</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
