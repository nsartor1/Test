<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Product_Cost__c</fullName>
    <description>Will show current Average Cost or the Override Cost if there is one, visible to all Sales Reps.</description>
    <externalId>false</externalId>
    <formula>IF( Override_Cost__c &gt; 0, Override_Cost__c,  Average_Cost__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Product Cost</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
