<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Est_Invoice_Date__c</fullName>
    <description>The estimated Invoice Date based on Closed Date, or End Date of Service Appts if Closed is Blank</description>
    <externalId>false</externalId>
    <formula>BLANKVALUE(Closed_Date__c,  DATEVALUE(EndDate))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Est Invoice Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Date</type>
</CustomField>
